# server/.htaccess
<IfModule mod_rewrite.c>
    RewriteEngine On
</IfModule>

Options -Indexes

<FilesMatch "^(config|\.env)">
    Order allow,deny
    Deny from all
</FilesMatch>

php_value upload_max_filesize 10M
php_value post_max_size 10M
php_value max_execution_time 300
php_value max_input_time 300